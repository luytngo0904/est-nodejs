{
  "name": "est-nodejs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "start": "nodemon ./bin/www",
    "build": "rimraf dist/ && babel src --out-dir dist/src --ignore node_modules --copy-files"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.11.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.7",
    "morgan": "~1.9.1",
    "nodemailer": "^6.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "babel-preset-minify": "^0.5.1",
    "nodemon": "^2.0.7"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": "3.6.5",
          "targets": {
            "node": "current"
          }
        }
      ],
      [
        "minify",
        {
          "builtIns": false,
          "mangle": {
            "keepFnName": false
          },
          "deadcode": {
            "keepFnName": false
          }
        }
      ]
    ]
  }
}
